{
  "name": "material-dashboard-react",
  "version": "1.7.0",
  "description": "Material Dashboard React. Coded by Creative Tim",
  "private": false,
  "main": "index.js",
  "dependencies": {
    "prop-types": "15.7.2",
    "react-google-maps": "9.4.5",
    "react-scripts": "3.0.1",
    "@material-ui/core": "4.1.0"
  },
  "peerDependencies": {
    "classnames": "2.2.6",
    "react": "16.8.6",
    "react-chartist": "0.13.3",
    "react-dom": "16.8.6",
    "history": "4.9.0",
    "react-router-dom": "5.0.1",
    "chartist": "0.10.1",
    "perfect-scrollbar": "1.4.0",
    "react-swipeable-views": "0.13.3",
    "@material-ui/icons": "4.1.0",
    "@material-ui/core": "4.1.0"
  },
  "scripts": {
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint:check": "eslint . --ext=js,jsx;  exit 0",
    "lint:fix": "eslint . --ext=js,jsx --fix;  exit 0",
    "copy-type-definitions": "cp -R types dist/types",
    "copy-package-file": "cp package.json package-lock.json dist/ ",
    "build-package": "mkdir -p dist && npm run copy-package-file && npm run copy-type-definitions && NODE_ENV=production babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/material-dashboard-react.git"
  },
  "keywords": [],
